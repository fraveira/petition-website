<div class="loginform">
{{#each rows}}
<form method="POST">
    <div class="form-group">
    <input type="text" name="first" class="form-control" value="{{first}}">
    </div>
      <div class="form-group">
    <input type="text" name="last" class="form-control" value="{{last}}">
    </div>
      <div class="form-group">
    <input type="email" name="email" class="form-control" value="{{email}}">
    </div>
    <div class="form-group">
        <input type="password" name="password" class="form-control" >
    </div>
    <div class="form-group">
    <input type="number" name="age" class="form-control" value="{{age}}">
    </div>
  <div class="form-group">
    <input type="text" name="city" class="form-control" value="{{city}}">
    </div>
      <div class="form-group">
    <input type="url" name="url" class="form-control" value="{{url}}">
    </div>

    {{/each}}

        <div class="form-group">
        <input type="hidden" name="_csrf" value="{{csrfToken}}">
  </div>
<button class="registerbtn" id="updates">UPDATE</button>
</form>
</div>
